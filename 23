-- ============================================
-- ФУНКЦИОНАЛЬНОСТЬ СЛАЙДЕРА
-- ============================================

local SliderZone = ElementZone:FindFirstChild("Section2"):FindFirstChild("SliderZone")
if SliderZone then
    local SliderLine = SliderZone:FindFirstChild("SliderLine")
    local SliderDot = SliderLine and SliderLine:FindFirstChild("Frame")
    local SliderValue = SliderZone:FindFirstChild("SliderValue")
    local ValueLabel = SliderValue and SliderValue:FindFirstChild("TextLabel")
    
    if SliderLine and SliderDot and ValueLabel then
        local minValue = 0
        local maxValue = 100
        local currentValue = 100
        local isDragging = false
        
        local function UpdateSlider(value)
            currentValue = math.clamp(value, minValue, maxValue)
            local percentage = (currentValue - minValue) / (maxValue - minValue)
            
            -- Обновляем позицию точки
            local maxPosition = SliderLine.AbsoluteSize.X - SliderDot.AbsoluteSize.X
            SliderDot.Position = UDim2.new(0, percentage * maxPosition, -0.75, 0)
            
            -- Обновляем значение
            ValueLabel.Text = tostring(math.floor(currentValue))
        end
        
        SliderLine.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                isDragging = true
                local mousePos = UserInputService:GetMouseLocation()
                local sliderPos = SliderLine.AbsolutePosition
                local sliderSize = SliderLine.AbsoluteSize.X
                local relativeX = math.clamp(mousePos.X - sliderPos.X, 0, sliderSize)
                local percentage = relativeX / sliderSize
                UpdateSlider(minValue + (maxValue - minValue) * percentage)
            end
        end)
        
        UserInputService.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                isDragging = false
            end
        end)
        
        UserInputService.InputChanged:Connect(function(input)
            if isDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                local mousePos = UserInputService:GetMouseLocation()
                local sliderPos = SliderLine.AbsolutePosition
                local sliderSize = SliderLine.AbsoluteSize.X
                local relativeX = math.clamp(mousePos.X - sliderPos.X, 0, sliderSize)
                local percentage = relativeX / sliderSize
                UpdateSlider(minValue + (maxValue - minValue) * percentage)
            end
        end)
    end
end

-- ============================================
-- ФУНКЦИОНАЛЬНОСТЬ БИНДА
-- ============================================

local BindZone = ElementZone:FindFirstChild("Section2"):FindFirstChild("BindZone")
if BindZone then
    local BindFrame = BindZone:FindFirstChild("Bind")
    local BindLabel = BindFrame and BindFrame:FindFirstChild("BindName")
    
    if BindFrame and BindLabel then
        local currentBind = Enum.KeyCode.B
        local listening = false
        
        -- Создаем невидимую кнопку поверх Frame для обработки кликов
        local ClickButton = Instance.new("TextButton")
        ClickButton.Name = "ClickButton"
        ClickButton.Size = UDim2.new(1, 0, 1, 0)
        ClickButton.Position = UDim2.new(0, 0, 0, 0)
        ClickButton.BackgroundTransparency = 1
        ClickButton.Text = ""
        ClickButton.TextTransparency = 1
        ClickButton.ZIndex = BindFrame.ZIndex + 1
        ClickButton.Parent = BindFrame
        
        ClickButton.MouseButton1Click:Connect(function()
            listening = true
            BindLabel.Text = "..."
            BindLabel.TextColor3 = Color3.fromRGB(150, 17, 255)
        end)
        
        UserInputService.InputBegan:Connect(function(input, gameProcessed)
            if listening and not gameProcessed then
                if input.KeyCode ~= Enum.KeyCode.Unknown then
                    currentBind = input.KeyCode
                    BindLabel.Text = input.KeyCode.Name
                    BindLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                    listening = false
                    CreateNotification("Бинд установлен: " .. input.KeyCode.Name)
                end
            end
        end)
        
        -- Проверка нажатия бинда
        UserInputService.InputBegan:Connect(function(input, gameProcessed)
            if not gameProcessed and input.KeyCode == currentBind then
                CreateNotification("Бинд активирован!")
                -- Здесь добавьте свою функциональность
            end
        end)
    end
end

-- ============================================
-- ФУНКЦИОНАЛЬНОСТЬ ДРОПДАУНА
-- ============================================

local DropdownZone = ElementZone:FindFirstChild("Section3"):FindFirstChild("DropdownZone")
if DropdownZone then
    local DropdownButton = DropdownZone:FindFirstChild("DropdownButton")
    local SelectedLabel = DropdownButton and DropdownButton:FindFirstChild("Selected")
    local Arrow = DropdownButton and DropdownButton:FindFirstChild("Arrow")
    
    if DropdownButton and SelectedLabel then
        local options = {"Option 1", "Option 2", "Option 3", "Goida, svo, zov. lolkekokak"}
        local selectedIndex = 4
        local isOpen = false
        
        local function CreateDropdownList()
            if isOpen then return end
            isOpen = true
            
            -- Создаем список опций
            local DropdownList = Instance.new("Frame")
            DropdownList.Name = "DropdownList"
            DropdownList.Size = UDim2.new(0, DropdownButton.AbsoluteSize.X, 0, #options * 25)
            DropdownList.Position = UDim2.new(0, DropdownButton.AbsolutePosition.X - DropdownZone.AbsolutePosition.X, 0, DropdownButton.AbsoluteSize.Y + 5)
            DropdownList.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
            DropdownList.BorderSizePixel = 0
            DropdownList.Parent = DropdownZone
            
            local UICorner = Instance.new("UICorner")
            UICorner.CornerRadius = UDim.new(0, 4)
            UICorner.Parent = DropdownList
            
            local UIStroke = Instance.new("UIStroke")
            UIStroke.Color = Color3.fromRGB(70, 70, 70)
            UIStroke.Parent = DropdownList
            
            for i, option in ipairs(options) do
                local OptionButton = Instance.new("TextButton")
                OptionButton.Size = UDim2.new(1, 0, 0, 25)
                OptionButton.Position = UDim2.new(0, 0, 0, (i - 1) * 25)
                OptionButton.BackgroundColor3 = i == selectedIndex and Color3.fromRGB(40, 40, 40) or Color3.fromRGB(29, 29, 29)
                OptionButton.BorderSizePixel = 0
                OptionButton.Text = option
                OptionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                OptionButton.TextSize = 12
                OptionButton.Font = Enum.Font.SourceSansBold
                OptionButton.Parent = DropdownList
                
                OptionButton.MouseButton1Click:Connect(function()
                    selectedIndex = i
                    SelectedLabel.Text = option
                    DropdownList:Destroy()
                    isOpen = false
                    CreateNotification("Выбрано: " .. option)
                end)
                
                OptionButton.MouseEnter:Connect(function()
                    if i ~= selectedIndex then
                        OptionButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
                    end
                end)
                
                OptionButton.MouseLeave:Connect(function()
                    if i ~= selectedIndex then
                        OptionButton.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
                    end
                end)
            end
        end
        
        -- Создаем невидимую кнопку поверх DropdownButton для обработки кликов
        local DropdownClickButton = Instance.new("TextButton")
        DropdownClickButton.Name = "DropdownClickButton"
        DropdownClickButton.Size = UDim2.new(1, 0, 1, 0)
        DropdownClickButton.Position = UDim2.new(0, 0, 0, 0)
        DropdownClickButton.BackgroundTransparency = 1
        DropdownClickButton.Text = ""
        DropdownClickButton.TextTransparency = 1
        DropdownClickButton.ZIndex = DropdownButton.ZIndex + 1
        DropdownClickButton.Parent = DropdownButton
        
        DropdownClickButton.MouseButton1Click:Connect(function()
            if isOpen then
                local list = DropdownZone:FindFirstChild("DropdownList")
                if list then list:Destroy() end
                isOpen = false
            else
                CreateDropdownList()
            end
        end)
        
        -- Закрытие при клике вне дропдауна
        UserInputService.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 and isOpen then
                local list = DropdownZone:FindFirstChild("DropdownList")
                if list and not DropdownButton:IsDescendantOf(game.Players.LocalPlayer.PlayerGui) then
                    list:Destroy()
                    isOpen = false
                end
            end
        end)
    end
end

-- ============================================
-- ГОРЯЧАЯ КЛАВИША ДЛЯ ОТКРЫТИЯ/ЗАКРЫТИЯ UI
-- ============================================

local UIEnabled = true
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.RightShift and MainFrame then
        UIEnabled = not UIEnabled
        MainFrame.Visible = UIEnabled
        
        if UIEnabled then
            CreateNotification("UI открыт")
        else
            CreateNotification("UI закрыт")
        end
    end
end)

-- ============================================
-- БИБЛИОТЕКА API
-- ============================================

local Library = {}

-- Хранилище данных
Library.Tabs = {}
Library.Sections = {}
Library.Elements = {}
Library.ToggleStates = {}

-- Сохраняем ссылки на UI элементы в библиотеке
Library.UI = UI
Library.MainFrame = MainFrame
Library.TabZone = TabZone
Library.ElementZone = ElementZone
Library.OpenedTab = OpenedTab
Library.CurrentTab = CurrentTab
Library.TabContents = TabContents
Library.CreateNotification = CreateNotification
Library.SwitchTab = SwitchTab
Library.UpdateToggle = UpdateToggle
Library.TweenService = TweenService

-- Методы для работы с табами
function Library:CreateTab(name)
    if Library.Tabs[name] then
        warn("Tab '" .. name .. "' already exists!")
        return Library.Tabs[name]
    end
    
    local tabIndex = #Library.Tabs + 1
    local tabY = 0.0535714291036129 + (tabIndex - 1) * 0.0892857201397419
    
    local NewTab = Instance.new("TextButton")
    NewTab.Name = "Tab" .. tabIndex
    NewTab.Size = UDim2.new(0, 120, 0, 25)
    NewTab.Position = UDim2.new(0.06428571790456772, 0, tabY, 0)
    NewTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    NewTab.BackgroundTransparency = tabIndex == 1 and 0.9 or 1
    NewTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
    NewTab.BorderSizePixel = 0
    NewTab.Text = ""
    NewTab.TextColor3 = Color3.fromRGB(0, 0, 0)
    NewTab.TextSize = 14
    NewTab.Font = Enum.Font.SourceSans
    NewTab.AnchorPoint = Vector2.new(0, 0)
    
    local UICorner = Instance.new("UICorner")
    UICorner.Parent = NewTab
    
    local TabLogo = Instance.new("ImageLabel")
    TabLogo.Name = "TabLogo"
    TabLogo.Size = UDim2.new(0, 14, 0, 14)
    TabLogo.Position = UDim2.new(0.06666667014360428, 0, 0.20000000298023224, 0)
    TabLogo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TabLogo.BackgroundTransparency = 1
    TabLogo.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TabLogo.BorderSizePixel = 0
    TabLogo.Image = "rbxassetid://12614416478"
    TabLogo.ImageColor3 = tabIndex == 1 and Color3.fromRGB(150, 17, 255) or Color3.fromRGB(161, 161, 161)
    TabLogo.AnchorPoint = Vector2.new(0, 0)
    TabLogo.Parent = NewTab
    
    local TabName = Instance.new("TextLabel")
    TabName.Name = "TabName"
    TabName.Size = UDim2.new(0, 89, 0, 23)
    TabName.Position = UDim2.new(0.25, 0, -0.03999878093600273, 0)
    TabName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TabName.BackgroundTransparency = 1
    TabName.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TabName.BorderSizePixel = 0
    TabName.Text = name
    TabName.TextColor3 = tabIndex == 1 and Color3.fromRGB(150, 17, 255) or Color3.fromRGB(161, 161, 161)
    TabName.TextSize = 14
    TabName.Font = Enum.Font.SourceSansBold
    TabName.AnchorPoint = Vector2.new(0, 0)
    TabName.Parent = NewTab
    
    NewTab.Parent = Library.TabZone
    
    -- Добавляем в TabContents
    table.insert(Library.TabContents, {Name = name, Elements = {}})
    
    -- Подключаем обработчик
    NewTab.MouseButton1Click:Connect(function()
        Library.SwitchTab(tabIndex)
    end)
    
    NewTab.MouseEnter:Connect(function()
        if Library.CurrentTab ~= tabIndex then
            NewTab.BackgroundTransparency = 0.92
        end
    end)
    
    NewTab.MouseLeave:Connect(function()
        if Library.CurrentTab ~= tabIndex then
            NewTab.BackgroundTransparency = tabIndex == 1 and 0.9 or 1
        end
    end)
    
    Library.Tabs[name] = {
        Index = tabIndex,
        Button = NewTab,
        Sections = {}
    }
    
    return Library.Tabs[name]
end

-- Методы для работы с секциями
function Library:CreateSection(tabName, sectionName, position)
    local tab = Library.Tabs[tabName]
    if not tab then
        warn("Tab '" .. tabName .. "' not found!")
        return nil
    end
    
    if Library.Sections[tabName] and Library.Sections[tabName][sectionName] then
        warn("Section '" .. sectionName .. "' already exists in tab '" .. tabName .. "'!")
        return Library.Sections[tabName][sectionName]
    end
    
    if not Library.Sections[tabName] then
        Library.Sections[tabName] = {}
    end
    
    position = position or {X = 0.5331807732582092, Y = 0.06666668504476547}
    
    local Section = Instance.new("Frame")
    Section.Name = sectionName
    Section.Size = UDim2.new(0, 210, 0, 100)
    Section.Position = UDim2.new(position.X, 0, position.Y, 0)
    Section.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
    Section.BackgroundTransparency = 0.6000000238418579
    Section.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Section.BorderSizePixel = 0
    Section.AnchorPoint = Vector2.new(0, 0)
    Section.Visible = tab.Index == CurrentTab
    
    local UICorner = Instance.new("UICorner")
    UICorner.Parent = Section
    
    local UIStroke = Instance.new("UIStroke")
    UIStroke.Parent = Section
    
    local SectionNameLabel = Instance.new("TextLabel")
    SectionNameLabel.Name = "SectionName"
    SectionNameLabel.Size = UDim2.new(0, 78, 0, 19)
    SectionNameLabel.Position = UDim2.new(0.037837836891412735, 0, -0.1933334320783615, 0)
    SectionNameLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    SectionNameLabel.BackgroundTransparency = 1
    SectionNameLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
    SectionNameLabel.BorderSizePixel = 0
    SectionNameLabel.Text = sectionName
    SectionNameLabel.TextColor3 = Color3.fromRGB(161, 161, 161)
    SectionNameLabel.TextSize = 12
    SectionNameLabel.Font = Enum.Font.SourceSansBold
    SectionNameLabel.AnchorPoint = Vector2.new(0, 0)
    SectionNameLabel.Parent = Section
    
    Section.Parent = Library.ElementZone
    
    Library.Sections[tabName][sectionName] = {
        Frame = Section,
        Elements = {}
    }
    
    tab.Sections[sectionName] = Library.Sections[tabName][sectionName]
    
    return Library.Sections[tabName][sectionName]
end

-- Методы для создания элементов
function Library:CreateButton(section, name, callback)
    if not section or not section.Frame then
        warn("Invalid section!")
        return nil
    end
    
    local ButtonZone = Instance.new("Frame")
    ButtonZone.Name = "ButtonZone_" .. name
    ButtonZone.Size = UDim2.new(0, 210, 0, 35)
    ButtonZone.Position = UDim2.new(0, 0, #section.Elements * 0.35, 0)
    ButtonZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ButtonZone.BackgroundTransparency = 1
    ButtonZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    ButtonZone.BorderSizePixel = 0
    ButtonZone.AnchorPoint = Vector2.new(0, 0)
    
    local Button = Instance.new("TextButton")
    Button.Size = UDim2.new(0, 90, 0, 25)
    Button.Position = UDim2.new(0.06666667014360428, 0, 0.13809552788734436, 0)
    Button.BackgroundColor3 = Color3.fromRGB(150, 17, 255)
    Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Button.BorderSizePixel = 0
    Button.Text = name
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextSize = 15
    Button.Font = Enum.Font.SourceSansBold
    Button.AnchorPoint = Vector2.new(0, 0)
    
    local UICorner = Instance.new("UICorner")
    UICorner.CornerRadius = UDim.new(0, 5)
    UICorner.Parent = Button
    
    Button.MouseButton1Click:Connect(function()
        if callback then callback() end
        Library.CreateNotification(name .. " нажата!")
    end)
    
    Button.MouseEnter:Connect(function()
        Library.TweenService:Create(Button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(170, 37, 255)
        }):Play()
    end)
    
    Button.MouseLeave:Connect(function()
        Library.TweenService:Create(Button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(150, 17, 255)
        }):Play()
    end)
    
    Button.Parent = ButtonZone
    ButtonZone.Parent = section.Frame
    
    table.insert(section.Elements, ButtonZone)
    
    return Button
end

function Library:CreateToggle(section, name, callback, defaultState)
    if not section or not section.Frame then
        warn("Invalid section!")
        return nil
    end
    
    defaultState = defaultState or false
    
    local ToggleZone = Instance.new("Frame")
    ToggleZone.Name = "ToggleZone_" .. name
    ToggleZone.Size = UDim2.new(0, 210, 0, 30)
    ToggleZone.Position = UDim2.new(0, 0, #section.Elements * 0.3, 0)
    ToggleZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ToggleZone.BackgroundTransparency = 1
    ToggleZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    ToggleZone.BorderSizePixel = 0
    ToggleZone.AnchorPoint = Vector2.new(0, 0)
    
    local ToggleName = Instance.new("TextLabel")
    ToggleName.Name = "ToggleName"
    ToggleName.Size = UDim2.new(0, 79, 0, 30)
    ToggleName.Position = UDim2.new(0.07027027010917664, 0, 0, 0)
    ToggleName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ToggleName.BackgroundTransparency = 1
    ToggleName.BorderColor3 = Color3.fromRGB(0, 0, 0)
    ToggleName.BorderSizePixel = 0
    ToggleName.Text = name
    ToggleName.TextColor3 = Color3.fromRGB(161, 161, 161)
    ToggleName.TextSize = 12
    ToggleName.Font = Enum.Font.SourceSansBold
    ToggleName.AnchorPoint = Vector2.new(0, 0)
    ToggleName.Parent = ToggleZone
    
    local Toggle = Instance.new("TextButton")
    Toggle.Name = "Toggle"
    Toggle.Size = UDim2.new(0, 30, 0, 16)
    Toggle.Position = UDim2.new(0.7610037922859192, 0, 0.23333333432674408, 0)
    Toggle.BackgroundColor3 = defaultState and Color3.fromRGB(150, 17, 255) or Color3.fromRGB(29, 29, 29)
    Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Toggle.BorderSizePixel = 0
    Toggle.Text = ""
    Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
    Toggle.TextSize = 14
    Toggle.Font = Enum.Font.SourceSans
    Toggle.AnchorPoint = Vector2.new(0, 0)
    
    local UICorner = Instance.new("UICorner")
    UICorner.Parent = Toggle
    
    local ToggleMover = Instance.new("TextButton")
    ToggleMover.Name = "ToggleMover"
    ToggleMover.Size = UDim2.new(0, 12, 0, 12)
    ToggleMover.Position = defaultState and UDim2.new(0.44999998807907104, 2, 0.125, 0) or UDim2.new(0, 2, 0.125, 0)
    ToggleMover.BackgroundColor3 = defaultState and Color3.fromRGB(255, 255, 255) or Color3.fromRGB(115, 115, 115)
    ToggleMover.BorderColor3 = Color3.fromRGB(0, 0, 0)
    ToggleMover.BorderSizePixel = 0
    ToggleMover.Text = ""
    ToggleMover.TextColor3 = Color3.fromRGB(0, 0, 0)
    ToggleMover.TextSize = 14
    ToggleMover.Font = Enum.Font.SourceSans
    ToggleMover.AnchorPoint = Vector2.new(0, 0)
    
    local UICorner2 = Instance.new("UICorner")
    UICorner2.Parent = ToggleMover
    ToggleMover.Parent = Toggle
    
    Toggle.MouseButton1Click:Connect(function()
        local state = Library.UpdateToggle(Toggle)
        Library.ToggleStates[Toggle] = state
        if callback then callback(state) end
        Library.CreateNotification(name .. " " .. (state and "включен" or "выключен"))
    end)
    
    Toggle.Parent = ToggleZone
    ToggleZone.Parent = section.Frame
    
    Library.ToggleStates[Toggle] = defaultState
    
    table.insert(section.Elements, ToggleZone)
    
    return Toggle
end

-- Обновляем SwitchTab для скрытия/показа секций
local originalSwitchTab = Library.SwitchTab
Library.SwitchTab = function(tabNumber)
    originalSwitchTab(tabNumber)
    
    -- Скрываем все секции
    for tabName, sections in pairs(Library.Sections) do
        for sectionName, section in pairs(sections) do
            if section.Frame then
                section.Frame.Visible = false
            end
        end
    end
    
    -- Показываем секции текущего таба
    local currentTabName = Library.TabContents[tabNumber].Name
    if Library.Sections[currentTabName] then
        for sectionName, section in pairs(Library.Sections[currentTabName]) do
            if section.Frame then
                section.Frame.Visible = true
            end
        end
    end
end

-- Инициализация
CreateNotification("UI загружен успешно!")
print("PizdecWare UI Library загружена!")

-- Возвращаем библиотеку
return Library
