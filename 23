-- ============================================
-- СИСТЕМА УВЕДОМЛЕНИЙ
-- ============================================

local function CreateNotification(text, duration)
    duration = duration or 3
    
    local NotifiList = Notifi:FindFirstChild("NotifiList")
    if not NotifiList then return end
    
    local NotificationTemplate = NotifiList:FindFirstChild("Notification1")
    if not NotificationTemplate then return end
    
    local NewNotification = NotificationTemplate:Clone()
    NewNotification.Name = "Notification_" .. tick()
    NewNotification.NotifiText.Text = text
    NewNotification.Position = UDim2.new(-0.0014721680199727416, 0, 0.8278283476829529, 0)
    NewNotification.Visible = true
    NewNotification.Parent = NotifiList
    
    -- Анимация появления
    local SlideIn = TweenService:Create(
        NewNotification,
        TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {Position = UDim2.new(-0.0014721680199727416, 0, 0.8278283476829529, 0)}
    )
    SlideIn:Play()
    
    -- Автоматическое удаление
    game:GetService("Debris"):AddItem(NewNotification, duration)
end

-- ============================================
-- DRAG ФУНКЦИОНАЛЬНОСТЬ ДЛЯ ОКНА
-- ============================================

local dragging = false
local dragInput, dragStart, startPos

local function update(input)
    if not MainFrame then return end
    local delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(
        startPos.X.Scale, startPos.X.Offset + delta.X,
        startPos.Y.Scale, startPos.Y.Offset + delta.Y
    )
end

if MainFrame then
    MainFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)

    MainFrame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
end

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

-- ============================================
-- ФУНКЦИОНАЛЬНОСТЬ ВКЛАДОК
-- ============================================

local CurrentTab = 1
local TabContents = {
    [1] = {Name = "Combat", Elements = {}},
    [2] = {Name = "Visuals", Elements = {}},
    [3] = {Name = "Misc", Elements = {}},
    [4] = {Name = "World", Elements = {}},
    [5] = {Name = "Settings", Elements = {}}
}

local function SwitchTab(tabNumber)
    if not OpenedTab then return end
    
    CurrentTab = tabNumber
    OpenedTab.Text = TabContents[tabNumber] and TabContents[tabNumber].Name or "Unknown"
    
    -- Обновляем видимость секций (можно настроить под ваши нужды)
    -- Здесь просто обновляем текст открытой вкладки
    
    -- Обновляем стили вкладок
    -- Сначала обновляем старые табы (Tab1-Tab5)
    local oldTabs = {Tab1, Tab2, Tab3, Tab4, Tab5}
    for i, tab in ipairs(oldTabs) do
        if tab then
            local TabName = tab:FindFirstChild("TabName")
            local TabLogo = tab:FindFirstChild("TabLogo")
            
            if i == tabNumber then
                -- Выбранная вкладка
                tab.BackgroundTransparency = 0.9
                if TabName then TabName.TextColor3 = Color3.fromRGB(150, 17, 255) end
                if TabLogo then TabLogo.ImageColor3 = Color3.fromRGB(150, 17, 255) end
            else
                -- Невыбранная вкладка
                tab.BackgroundTransparency = 0.95
                if TabName then TabName.TextColor3 = Color3.fromRGB(255, 255, 255) end
                if TabLogo then TabLogo.ImageColor3 = Color3.fromRGB(255, 255, 255) end
            end
        end
    end
    
    -- Затем обновляем новые табы из Library
    for tabName, tabData in pairs(Library.Tabs) do
        if tabData.Button then
            local TabName = tabData.Button:FindFirstChild("TabName")
            local TabLogo = tabData.Button:FindFirstChild("TabLogo")
            
            if tabData.Index == tabNumber then
                -- Выбранная вкладка
                tabData.Button.BackgroundTransparency = 0.9
                if TabName then TabName.TextColor3 = Color3.fromRGB(150, 17, 255) end
                if TabLogo then TabLogo.ImageColor3 = Color3.fromRGB(150, 17, 255) end
            else
                -- Невыбранная вкладка
                tabData.Button.BackgroundTransparency = tabData.Index == 1 and 0.9 or 1
                if TabName then TabName.TextColor3 = Color3.fromRGB(255, 255, 255) end
                if TabLogo then TabLogo.ImageColor3 = Color3.fromRGB(255, 255, 255) end
            end
        end
    end
    
    CreateNotification("Переключено на " .. TabContents[tabNumber].Name)
end

if Tab1 then
    Tab1.MouseButton1Click:Connect(function()
        SwitchTab(1)
    end)
end

if Tab2 then
    Tab2.MouseButton1Click:Connect(function()
        SwitchTab(2)
    end)
    
    -- Hover эффекты для вкладок
    Tab2.MouseEnter:Connect(function()
        if CurrentTab ~= 2 then
            Tab2.BackgroundTransparency = 0.92
        end
    end)
    
    Tab2.MouseLeave:Connect(function()
        if CurrentTab ~= 2 then
            Tab2.BackgroundTransparency = 0.95
        end
    end)
end

if Tab3 then
    Tab3.MouseButton1Click:Connect(function()
        SwitchTab(3)
    end)
    
    Tab3.MouseEnter:Connect(function()
        if CurrentTab ~= 3 then
            Tab3.BackgroundTransparency = 0.92
        end
    end)
    
    Tab3.MouseLeave:Connect(function()
        if CurrentTab ~= 3 then
            Tab3.BackgroundTransparency = 1
        end
    end)
end

if Tab4 then
    Tab4.MouseButton1Click:Connect(function()
        SwitchTab(4)
    end)
    
    Tab4.MouseEnter:Connect(function()
        if CurrentTab ~= 4 then
            Tab4.BackgroundTransparency = 0.92
        end
    end)
    
    Tab4.MouseLeave:Connect(function()
        if CurrentTab ~= 4 then
            Tab4.BackgroundTransparency = 1
        end
    end)
end

if Tab5 then
    Tab5.MouseButton1Click:Connect(function()
        SwitchTab(5)
    end)
    
    Tab5.MouseEnter:Connect(function()
        if CurrentTab ~= 5 then
            Tab5.BackgroundTransparency = 0.92
        end
    end)
    
    Tab5.MouseLeave:Connect(function()
        if CurrentTab ~= 5 then
            Tab5.BackgroundTransparency = 1
        end
    end)
end

-- ============================================
-- ФУНКЦИОНАЛЬНОСТЬ ТОГГЛОВ
-- ============================================

local ToggleStates = {}

local function UpdateToggle(toggleButton, state)
    local ToggleMover = toggleButton:FindFirstChild("ToggleMover")
    if not ToggleMover then return end
    
    state = state or not ToggleStates[toggleButton]
    ToggleStates[toggleButton] = state
    
    if state then
        -- Включено
        toggleButton.BackgroundColor3 = Color3.fromRGB(150, 17, 255)
        TweenService:Create(
            ToggleMover,
            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Position = UDim2.new(0.44999998807907104, 2, 0.125, 0)}
        ):Play()
        ToggleMover.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    else
        -- Выключено
        toggleButton.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
        TweenService:Create(
            ToggleMover,
            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
            {Position = UDim2.new(0, 2, 0.125, 0)}
        ):Play()
        ToggleMover.BackgroundColor3 = Color3.fromRGB(115, 115, 115)
    end
    
    return state
end

-- Toggle 1
local Toggle1Zone = ElementZone:FindFirstChild("Section1"):FindFirstChild("Toggle 1Zone")
if Toggle1Zone then
    local Toggle1 = Toggle1Zone:FindFirstChild("Toggle")
    local ToggleName1 = Toggle1Zone:FindFirstChild("ToggleName")
    
    if Toggle1 then
        Toggle1.MouseButton1Click:Connect(function()
            local state = UpdateToggle(Toggle1)
            CreateNotification((ToggleName1 and ToggleName1.Text or "Toggle 1") .. " " .. (state and "включен" or "выключен"))
        end)
    end
end

-- Toggle 2 (уже включен)
local Toggle2Zone = ElementZone:FindFirstChild("Section1"):FindFirstChild("Toggle 2Zone(enabled)")
if Toggle2Zone then
    local Toggle2 = Toggle2Zone:FindFirstChild("Toggle")
    local ToggleName2 = Toggle2Zone:FindFirstChild("ToggleName")
    
    if Toggle2 then
        ToggleStates[Toggle2] = true -- Уже включен по умолчанию
        
        Toggle2.MouseButton1Click:Connect(function()
            local state = UpdateToggle(Toggle2)
            CreateNotification((ToggleName2 and ToggleName2.Text or "Toggle 2") .. " " .. (state and "включен" or "выключен"))
        end)
    end
end

